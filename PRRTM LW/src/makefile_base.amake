
#
# Absoft Developer Tools Interface requires use of this makefile.
#
MKDIR=mkdir -p 
RMDIR=rm -rf 

#
# Variables for the compiler(s) and linker
#
ABSOFT_F95=f95
ABSOFT_F77=f95
VAST_PREPROCESSOR=vastfp
RC=Rez
GCC=gcc
MAKE=amake2
ABSOFT_F95_LINKER=f95
DUMMY=

#
# Flags
#
ABSOFT_F95_FLAGS= -O2 -m64 -s -stack:1f40000 -z0
ABSOFT_F77_FLAGS= -O2 -f77_compat -m64 -s -stack:1f40000 -z0
RC_FLAGS=
GCC_FLAGS= -O2 -m64
MAKE_FLAGS= -k
ABSOFT_F95_LINKER_FLAGS=-IPA:debug=on -IPA:source_pu_order=on       -stack:1f40000 -m64 -O2

#
# Name of the executable
#
TARGET=prrtmlw
OUTDIR=.
OBJDIR=./Release

#
# Files for this executable
#
VAST_PREPROCESSOR_FILES=
ABSOFT_F95_FILES=
ABSOFT_F77_FILES="ErrPack.f" "LINPAK.f" "RDI1MACH.f" "cldprop.f" "disort.f" "extra.f" "k_g.f" "rrtatm.f" "rtrdis.f" "setcoef.f" "taumol.f" "rrtm.f" "rtrcld.f" "rtrcldmr.f" "rtreg.f" "rtregcld.f" "rtregcldmr.f" "rtr.f"
GCC_FILES=
ABSOFT_F95_LINKER_FILES=
RC_FILES=
MAKE_FILES=

#
# Rules and dependencies for the executable
#
SRCFILES=$(ABSOFT_F77_FILES)
OBJFILES="$(OBJDIR)/ErrPack.o" "$(OBJDIR)/LINPAK.o" "$(OBJDIR)/RDI1MACH.o" "$(OBJDIR)/cldprop.o" "$(OBJDIR)/disort.o" "$(OBJDIR)/extra.o" "$(OBJDIR)/k_g.o" "$(OBJDIR)/rrtatm.o" "$(OBJDIR)/rtrdis.o" "$(OBJDIR)/setcoef.o" "$(OBJDIR)/taumol.o" "$(OBJDIR)/rrtm.o" "$(OBJDIR)/rtrcld.o" "$(OBJDIR)/rtrcldmr.o" "$(OBJDIR)/rtreg.o" "$(OBJDIR)/rtregcld.o" "$(OBJDIR)/rtregcldmr.o" "$(OBJDIR)/rtr.o"
all: "$(OBJDIR)" "$(OUTDIR)/$(TARGET)"
	

"$(OUTDIR)/$(TARGET)":  $(OBJFILES) $(ABSOFT_F95_LINKER_FILES) $(MODFILES)
	$(ABSOFT_F95_LINKER)  $(LPATHS) $(OBJFILES) $(ABSOFT_F95_LINKER_FILES) $(LIBS) -o "$(OUTDIR)/$(TARGET)" $(ABSOFT_F95_LINKER_FLAGS)



"$(OBJDIR)/ErrPack.o": "ErrPack.f"
	$(ABSOFT_F77) -c -nowdir -YLOOP_ANALYZER -LNO:simd_verbose=on -LNO:apo_verbose=on -CG:filetable_verbose=on $(ABSOFT_F77_FLAGS) -o "$(OBJDIR)/ErrPack.o" "ErrPack.f"

