"$(OBJDIR)":
	$(MKDIR) "$(OBJDIR)"
#
# Clean Rules
#
clean:
	$(RMDIR) "$(OBJDIR)"
	$(RMDIR) "$(TARGET)"
	rm -f makefile.amake
	rm -f makefile.amake.bk
	rm -f makefile_base.amake

#
# Define necessary macros for dependency scanning.
#
MKDEP=amakedepend
MKDEP_FLAGS= -quiet "-pre$(OBJDIR)/" -Y -modInfo -errdel -f90 -f makefile.amake
#
# Generate dependencies for the project
#
ScanAll: "$(OUTDIR)" "$(OBJDIR)" MakeDepsAll

#
# Scan for all F77/F95, and c/c++ dependencies
#
MakeDepsAll:
	$(MKDEP) -quiet "-pre./Release/" -Y -modInfo -errdel -f90 -f makefile.amake -a -info  -O2 -f77_compat -m64 -s -stack:1f40000 -z0 -info "ErrPack.f" "LINPAK.f" "RDI1MACH.f" "cldprop.f" "disort.f" "extra.f" "k_g.f" "rrtatm.f" "rtrdis.f" "setcoef.f" "taumol.f" "rrtm.f" "rtrcld.f" "rtrcldmr.f" "rtreg.f" "rtregcld.f" "rtregcldmr.f" "rtr.f" 
